{
  "manifest_version": 2,
  "name": "Netflix IMDB",
  "description": "What do you miss the most on the Netflix website? That's right, information from IMDB.com.",
  "version": "0.1.1",
  "permissions": [
    "*://*.netflix.com/",
    "webRequest"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.netflix.com/*"],
      "js": ["./build/mainScript.js"],
      "css" : ["./build/imdbNetflix.css"]
    }
  ]
}